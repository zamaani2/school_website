{
  "version": 2,
  "builds": [
    { "src": "api/wsgi.py", "use": "@vercel/python" },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/api/wsgi.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "school_website.production_settings"
  }
}
